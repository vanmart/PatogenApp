<%- model_class = Promonewsmultim -%>
<div class="page-header">
  <h1><%=t '.title', default: model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:promotion_id) %></th>
      <th><%= model_class.human_attribute_name(:multim_type) %></th>
      <th><%= model_class.human_attribute_name(:importantnews_id) %></th>
      <th><%= model_class.human_attribute_name(:reference) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', default: t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @promonewsmultims.each do |promonewsmultim| %>
      <tr>
        <td><%= promonewsmultim.id %></td>
          <td><%= promonewsmultim.promotion_id %></td>
          <td><%= promonewsmultim.multim_type %></td>
          <td><%= promonewsmultim.importantnews_id %></td>
          <td><%= promonewsmultim.reference %></td>
        <td><%=l promonewsmultim.created_at, format: :long %></td>
        <td>
          <%= link_to promonewsmultim_path(promonewsmultim), class: 'btn btn-xs', title: "#{ t('.show', default: t('helpers.links.show')) }" do %>
            <%= glyph 'info-sign' %>
          <%- end -%>
          <%= link_to edit_promonewsmultim_path(promonewsmultim), class: 'btn btn-xs', title: "#{ t('.edit', default: t('helpers.links.edit')) }" do %>
            <%= glyph 'pencil' %>
          <%- end -%>
          <%= link_to promonewsmultim_path(promonewsmultim), method: :delete, data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?')) }, class: 'btn btn-xs', title: "#{ t('.destroy', default: t('helpers.links.destroy')) }" do %>
            <%= glyph 'remove' %>
          <%- end -%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', default: t("helpers.links.new")),
            new_promonewsmultim_path,
            class: 'btn btn-primary' %>
