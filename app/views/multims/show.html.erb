<%- model_class = Multim -%>
<div class="page-header">
  <h1><%=t '.title', default: model_class.model_name.human.titleize %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:link) %></dt>
      <dd><pre class="prettyprint"><%= @multim.link %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:multim_type) %></dt>
      <dd><pre class="prettyprint"><%= @multim.multim_type %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:culture_id) %></dt>
      <dd><pre class="prettyprint"><%= @multim.culture_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:pathogen_id) %></dt>
      <dd><pre class="prettyprint"><%= @multim.pathogen_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:disease_id) %></dt>
      <dd><pre class="prettyprint"><%= @multim.disease_id %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', default: t("helpers.links.back")),
              multims_path, class: 'btn btn-default'  %>
  <%= link_to t('.edit', default: t("helpers.links.edit")),
              edit_multim_path(@multim), class: 'btn btn-default' %>
  <%= link_to t('.destroy', default: t("helpers.links.destroy")),
              multim_path(@multim),
              method: 'delete',
              data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?')) },
              class: 'btn btn-danger' %>
</div>